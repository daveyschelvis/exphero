<?xml version="1.0" encoding="utf-8"?>
<resources>



    <declare-styleable name="CalibrationImageButtonView">
        <attr name="foreground_color" format="color" />
        <attr name="background_color" format="color" />
        <attr name="widget_orientation">
            <enum name="above" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
            <enum name="below" value="4" />
        </attr>
        <attr name="calibration_circle_radius" format="dimension" />
    </declare-styleable>

</resources>